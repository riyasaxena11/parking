{{>header}}
    <title>All Record</title>
    {{>navbar}}
    <div class="container">
        <h5 class="bg-secondary text-light text-center mt-2 p-2">Park4U<a href="/add"><span class="material-symbols-outlined">add</span></a></h5>
        <div class="table-response">
            <table class="table">
                <tr>
                    <th>ID</th>
                    <th>Car_registration_no.</th>
                    <th>Owner</th>
                    <th>Date</th>
                    <th>Arrival time</th>
                    <th>Departure time</th>
                    <th>Slot</th>
                    <th>Fare</th>
                    <th></th>
                    <th></th>
                </tr>
                {{#each data}}
                <tr>
                    <td>{{_id}}</td>
                    <td>{{Car_Registration_number}}</td>
                    <td>{{Owner}}</td>
                    <td>{{Date}}</td>
                    <td>{{Arrival}}</td>
                    <td>{{Departure}}</td>
                    <td>{{slot}}</td>
                    <td>{{Fare}}</td>
                    <td><a href="/update/{{_id}}"><span class="material-symbols-outlined">edit</span></a></td>
                    <td><a href="/delete/{{_id}}"><span class="material-symbols-outlined">delete_forever</span></a></td>
                </tr>
                {{/each}}
            </table>
        </div>
    </div>
    {{>footer}}
